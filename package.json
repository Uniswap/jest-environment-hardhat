{
  "name": "@uniswap/jest-environment-hardhat",
  "version": "1.0.0",
  "description": "A jest environment with hardhat built in",
  "repository": "https://github.com/Uniswap/jest-environment-hardhat",
  "author": "Zach Pomerantz <zzmp@uniswap.org>",
  "license": "GPL-3.0-or-later",
  "main": "dist/node.js",
  "types": "dist/types/hardhat.d.ts",
  "scripts": {
    "prepare": "yarn compile",
    "prepublishOnly": "yarn build",
    "compile": "typechain --target ethers-v5 --out-dir src/types 'abis/**/*.json'",
    "build": "tsc",
    "release": "semantic-release",
    "lint": "eslint .",
    "test": "jest test/",
    "test:e2e": "jest e2e/ --testSequencer ./e2e/sequencer.js --maxWorkers 2"
  },
  "dependencies": {
    "setimmediate": "^1.0.5"
  },
  "peerDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "ethers": "^5.1.3",
    "hardhat": "^2.9.6",
    "jest": ">=27"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@ethersproject/abi": "^5.0.0",
    "@ethersproject/bytes": "^5.0.0",
    "@ethersproject/providers": "^5.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@semantic-release/git": "^10.0.1",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@uniswap/sdk-core": "^3.0.1",
    "chai": "^4.2.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ethers": "^5.1.3",
    "hardhat": "^2.9.6",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-environment-node": "^28.1.0",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  }
}
